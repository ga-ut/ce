{
  "name": "@ga-ut/ce",
  "version": "1.1.0",
  "description": "Custom Element library with state binding and optional routing",
  "license": "ISC",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "pack:check": "npm pack --dry-run"
  },
  "devDependencies": {
    "jsdom": "^25.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  }
}
